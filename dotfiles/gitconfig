[alias]
  s = status
  d = diff
  a = add
  co = checkout
  b = branch
  ba = branch -a
  l = log
  f = fetch
  r = reset
  p = pull
  pr = pull --rebase
  ps = push
  pom = push origin master
  cp = cherry-pick
  up = !git fetch origin && git rebase origin/master
[color]
  ui = auto
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
	precomposeunicode = true
[push]
	default = current
[rerere]
	enabled = 1
